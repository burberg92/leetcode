   "source": [
    "#Reversing a linked list \n",
    "\n",
    "def fn(head):\n",
    "    curr = head\n",
    "    prev = None\n",
    "    while curr:\n",
    "        next_node = curr.next\n",
    "        curr.next = prev\n",
    "        prev = curr\n",
    "        curr = next_node \n",
    "        \n",
    "    return prev"
   ]
